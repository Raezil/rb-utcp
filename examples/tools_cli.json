{
  "version": "1.0",
  "tools": [
    {
      "name": "shell_echo",
      "description": "Use local /bin/echo to print a message",
      "inputs": { "type": "object", "properties": { "msg": { "type": "string" } }, "required": ["msg"] },
      "outputs": { "type": "object" },
      "tags": ["cli", "echo"],
      "tool_provider": {
        "provider_type": "cli",
        "command": ["/bin/echo", "Message: ${msg}"]
      }
    }
  ]
}
